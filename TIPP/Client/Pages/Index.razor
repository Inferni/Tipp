@*@page "/"

    <Dashboard></Dashboard>*@

@page "/"
@attribute [Authorize]
@inject Service.IUserDataService userDataService
@using TIPP.Shared 


@switch (userDataService.User.Role)
{
    case Role.User:
        <Dashboard></Dashboard>
        break;
    case Role.Admin:
        <AdminDashboard></AdminDashboard>
        break;
    default:
        break;
}
